{
  "version": "1.0.49",
  "name": "@jetplane/velocity-tools",
  "description": "Velocity tools",
  "author": "WolfRayet",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jetplane/velocity-tools.git"
  },
  "module": "dist/velocity-tools.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=16",
    "npm": ">=9.5"
  },
  "scripts": {
    "build": "vite build",
    "start": "rollup -c -w",
    "test": "jest",
    "publish-patch": "npm run build && npm version patch && npm publish"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "react": "17.x || 18.x",
    "react-dom": "17.x || 18.x",
    "@meshsdk/core": "1.5.5"
  },
  "browser": {
    "@emurgo/cardano-message-signing-nodejs": "@emurgo/cardano-message-signing-browser",
    "@emurgo/cardano-serialization-lib-nodejs": "@emurgo/cardano-serialization-lib-browser"
  },
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@rollup/plugin-babel": "6.0.2",
    "@rollup/plugin-typescript": "9.0.2",
    "@types/styled-components": "5.1.26",
    "@vitejs/plugin-react": "2.2.0",
    "autoprefixer": "10.4.12",
    "babel-loader": "8.2.5",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-styled-components": "2.0.7",
    "postcss": "8.4.18",
    "rollup": "3.2.3",
    "tailwindcss": "3.2.1",
    "twin.macro": "3.0.0-rc.5",
    "vite": "3.1.4"
  },
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {
    "prop-types": "^15.7.2"
  }
}
