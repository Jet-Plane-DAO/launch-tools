{
  "name": "@jetplane/launch-tools",
  "version": "1.0.6",
  "description": "Jetplane Launch tools",
  "author": "WolfRayet",
  "license": "Apache-2.0",
  "repository": "jetplane/launch-tools",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=9.5"
  },
  "scripts": {
    "build": "rollup -c",
    "start": "rollup -c -w",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.4.1",
    "@types/jest": "^26.0.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "jest": "^29.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^1.29.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-typescript2": "^0.25.3",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^29.0.0",
    "typescript": "4.8.4"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "react hooks",
    "typescript",
    "npm"
  ],
  "dependencies": {
    "@meshsdk/core": "1.3.0",
    "@meshsdk/react": "1.1.3",
    "prop-types": "^15.7.2"
  }
